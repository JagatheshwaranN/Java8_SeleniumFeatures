			<groupId>com.browserup</groupId>
			<artifactId>browserup-proxy-core</artifactId>
			<version>2.1.2</version>
		</dependency>
		<dependency>
			<groupId>com.github.blibli-badak</groupId>
			<artifactId>selenium-har-util</artifactId>
			<version>1.0.2</version>
		</dependency>
